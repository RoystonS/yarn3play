{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2020",
    // "composite": true
    "module": "ES2022",
    "moduleResolution": "Node16",
    "lib": ["dom", "ESNext"],
    "allowJs": true,
    "checkJs": false,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "types": []
  }
}
